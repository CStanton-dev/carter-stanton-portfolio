{
  "name": "carterstanton.dev",
  "description": "Source code for https://carterstanton.dev",
  "version": "1.0.0",
  "private": true,
  "author": "Carter Stanton",
  "keywords": [
    "carter",
    "stanton",
    "dev"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public && rimraf .cache",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "format:gatsby": "prettier --write \"gatsby-*.js\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "now-build": "npm run build",
    "now-dev": "gatsby develop -p $PORT",
    "start": "npm run dev",
    "type-check": "tsc",
    "test": "npm run type-check"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/classnames": "^2.3.1",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@types/react-scroll": "^1.8.3",
    "classnames": "^2.3.1",
    "gatsby": "^4.3.0",
    "gatsby-link": "^4.3.0",
    "gatsby-plugin-canonical-urls": "^4.3.0",
    "gatsby-plugin-google-analytics": "^4.3.0",
    "gatsby-plugin-postcss": "^5.3.0",
    "gatsby-plugin-react-helmet": "^5.3.0",
    "gatsby-plugin-sass": "^5.3.0",
    "gatsby-plugin-typescript": "^4.3.0",
    "gatsby-source-filesystem": "^4.3.0",
    "gatsby-transformer-json": "^4.3.0",
    "normalize.css": "8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-scroll": "^1.8.4",
    "sass": "^1.45.0",
    "typeface-open-sans": "^1.1.13",
    "typeface-rubik": "^1.1.13",
    "typescript": "^4.5.3"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-blvd": "^1.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2"
  }
}
